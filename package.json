{
  "name": "videojs-vimeo",
  "version": "2.0.3",
  "description": "Videojs Vimeo official plugin fork using the official Vimeo Player API.",
  "main": "dist/videojs-vimeo.cjs.js",
  "module": "dist/videojs-vimeo.esm.js",
  "browser": "dist/videojs-vimeo.umd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "keywords": [
    "videojs",
    "videojs-plugin",
    "vimeo",
    "mobiliza"
  ],
  "author": "Kevin Aguiar <kevin.aguiar@mobiliza.com.br>",
  "license": "MIT",
  "style": "dist/videojs-vimeo.css",
  "videojs-plugin": {
    "style": "dist/videojs-vimeo.css",
    "script": "dist/videojs-vimeo.min.js"
  },
  "vjsstandard": {
    "ignore": [
      "dist",
      "scripts"
    ]
  },
  "files": [
    "dist/",
    "index.html",
    "src/"
  ],
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@vimeo/player": "^2.10.0",
    "browserify-versionify": "^1.0.6",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-json-light": "^1.0.3",
    "eslint-plugin-markdown": "^1.0.2",
    "rollup": "^1.32.0",
    "video.js": "^7.6.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-videojs": "^5.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mobiliza/videojs-vimeo.git"
  },
  "bugs": {
    "url": "https://github.com/Mobiliza/videojs-vimeo/issues"
  },
  "homepage": "https://github.com/Mobiliza/videojs-vimeo/#readme"
}
